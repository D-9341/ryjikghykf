using UnityEngine;

public class steve : MonoBehaviour {

	// Use this for initialization
	void Start () {
		GameObject.CreatePrimitive (PrimitiveType.Cube).transform.position = new Vector3 (0, 0, 0);
		GameObject.CreatePrimitive (PrimitiveType.Cube).transform.position = new Vector3 (-0.5f, 1, 0);
		GameObject.CreatePrimitive (PrimitiveType.Cube).transform.position = new Vector3 (-0.5f, 2, 0);
		GameObject.CreatePrimitive (PrimitiveType.Cube).transform.position = new Vector3 (0, 3, 0);
		GameObject.CreatePrimitive (PrimitiveType.Cube).transform.position = new Vector3 (0.5f, 2, 0);
		GameObject.CreatePrimitive (PrimitiveType.Cube).transform.position = new Vector3 (0.5f, 1, 0);
		GameObject.CreatePrimitive (PrimitiveType.Sphere).transform.position = new Vector3 (3, 5, 0);
		GameObject.CreatePrimitive (PrimitiveType.Quad).transform.position = new Vector3 (3, 2, 4);
		GameObject.CreatePrimitive (PrimitiveType.Plane).transform.position = new Vector3 (3, -3, 1);
		GameObject.CreatePrimitive (PrimitiveType.Cylinder).transform.position = new Vector3 (-3, 0, -4);
		GameObject.CreatePrimitive (PrimitiveType.Capsule).transform.position = new Vector3 (-3, 0, -2);
	}
	
}
